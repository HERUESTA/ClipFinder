<% @clips.each do |clip| %>
<li class="w-80 flex items-center gap-x-3.5 py-2 px-3 rounded-md text-sm text-gray-800 hover:bg-blue-200 focus:ring-2 focus:ring-blue-500 dark:text-gray-400 dark:hover:bg-blue-200 dark:hover:text-gray-300" role="option" data-autocomplete-value="<%= clip.id %>" data-autocomplete-label="<%= clip.streamer.streamer_name %>">
  <%= t('activerecord.attributes.clip.streamer.name', name: clip.streamer.streamer_name)  %>
</li>
<% end %>